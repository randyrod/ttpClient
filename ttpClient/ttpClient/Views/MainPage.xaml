<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="ttpClient.Views.MainPage">

    <MasterDetailPage.Master>
        <NavigationPage Title="" Icon="hamburger.png">
            <x:Arguments>
                <ContentPage Title="Menu">
                    <StackLayout Padding="40">
                        <Button Text="Contracts" Command="{Binding NavigationCommand}" CommandParameter="Navigation/ContractsPage"/>
                    </StackLayout>
                </ContentPage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Master>
    
</MasterDetailPage>
